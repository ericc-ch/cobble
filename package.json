{
  "name": "pkg-placeholder",
  "version": "0.0.0",
  "description": "_description_",
  "keywords": [
    "pkg-placeholder"
  ],
  "homepage": "https://github.com/ericc-ch/pkg-placeholder",
  "bugs": "https://github.com/ericc-ch/pkg-placeholder/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericc-ch/pkg-placeholder.git"
  },
  "author": "Erick Christian <erickchristian48@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "bin": {
    "pkg-placeholder": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "bun run --watch src/main.ts",
    "lint": "eslint .",
    "prepack": "bun run build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && bun publish --access public",
    "start": "bun run src/main.tsx",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  },
  "lint-staged": {
    "*": "bun run lint --fix"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.82.0",
    "ansi-escapes": "^7.0.0",
    "citty": "^0.1.6",
    "ignore": "^7.0.5",
    "ink": "^6.0.1",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.0",
    "tinyexec": "^1.0.1",
    "use-zustand": "^0.2.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@echristian/eslint-config": "^0.0.46",
    "@types/bun": "^1.2.18",
    "@types/react": "^19.1.8",
    "bumpp": "^10.2.0",
    "eslint": "^9.30.1",
    "lint-staged": "^16.1.2",
    "prettier-plugin-packagejson": "^2.5.18",
    "simple-git-hooks": "^2.13.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
